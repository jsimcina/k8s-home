machine:
  network:
    hostname: home-node-01
    interfaces:
      - interface: bond0
        bond:
          deviceSelectors: [{ hardwareAddr: "58:47:ca:77:f1:f7", driver: i40e },{ hardwareAddr: "58:47:ca:77:f1:f6", driver: i40e }]
          mode: 802.3ad
          xmitHashPolicy: layer3+4
          lacpRate: fast
          miimon: 1000
        mtu: 1500
        dhcp: true
        vip:
          ip: 10.12.0.254
